{
	"author": "sereneblue",
	"background": {
	    "scripts": ["js/background.js"]
	},
	"name": "Camo",
	"version": "0.1",
	"manifest_version": 2,
	"description": "Spoof your browser profile. Includes a few privacy enhancing options.",
	"browser_action": {
		"default_icon": "img/icon_48.png",
		"default_popup": "popup.html"
	},
	"icons": {
	    "48": "img/icon_48.png",
	    "96": "img/icon_96.png"
	},
	"permissions": [
		"http://*/*",
		"https://*/*",
		"<all_urls>",
		"alarms",
		"storage",
		"webRequest",
		"webRequestBlocking"
	]
}